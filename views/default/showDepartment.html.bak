{{extend 'layout.html'}}
<table>
  <tr>
    <td>{{=A('Manage Departments',_href=URL('manage'))}}</td>
  </tr>
  {{for dept in depts:}}
  <tr>
    <td>{{=A(dept.name,_href=URL('showCourse',args=dept.id))}}</td>
    <td>{{=dept.short_name}}</td>
    <td>{{=A('Edit',_href=URL('departmentEdit',args=dept.id))}}</td>
  </tr>
  {{pass}}
</table>
